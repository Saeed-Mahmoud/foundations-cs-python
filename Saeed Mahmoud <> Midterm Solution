    print("-----------------------------------")

def displayMenuUser():#O(1)
    print("\n-----------------------------------")
    print("1.Book Ticket\n" +
     "2.Exit")
  #this function is to display the menu to the user
    print("-----------------------------------")


def main():
  #This is the main function that will run the entire code
  import csv
  data_i_l=[]
  with open('Events.csv','r') as f:
   filecontent=csv.reader(f)
   for row in filecontent:
    data_i_l.append(row)
  f.close()
  date_keys=[]
  event_id=[]
  for i in range(len(data_i_l)):
    date_keys.append(data_i_l[i][3])
    event_id.append(data_i_l[i][1])


  
  print("Welcome to our Humble Ticketing system.")
  u_name=input("Enter username: ")
  adm="admin"
  if u_name==adm:
    count=0
    while (count != 6): #O(1) becuase the loop has a constant value
      p_word=input("Enter Password: ")
      pswrd="admin123123"
      if (count==5):
        count+=1
        print("You have reached the limit of attempts")
      elif p_word!=pswrd:
        count +=1
        print("Incorrect Password")
      else:
        displayMenuAdmin()
        choice = eval(input("Enter your choice:"))
        while (choice != 7):
          if choice == 1:
            displayStatistics(event_id)
          elif choice == 2:
            print("Under Construction")
          elif choice == 3:
            print("Under Construction")
          elif choice == 4:
            print("Under Construction")
          elif choice == 5:
            print("Under Construction")
          elif choice == 6:
            print("Under Construction")
          elif choice != 7:
            print("Your choice is invalid. Try again.")
          
          displayMenuAdmin()
          choice = eval(input("Enter your choice:"))

        print("\n---> You Exited!")
        break
    

  else:
    displayMenuUser()
    choice1=eval(input("Enter your choice:"))
    while (choice1 != 2):
      
      if choice1 == 1:
        print("Under Construction")
      elif choice1 != 2:
          print("Your choice is invalid. Try again.")
      displayMenuUser()
      choice1 = eval(input("Enter your choice:"))
    print("\n---> You Exited!")

main() 

