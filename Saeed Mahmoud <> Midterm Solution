def displayStatistics(event_id):
#this function finds the highest Number of tickets that is signed for each event by taking the events occurence list, putting them in a 
#set to remove all duplicates, making a list of the number of occurence for each event_id taken from that set. and then find the Event 
#with the highest number of tickets 
  set_events=set()
  for i in event_id:
    set_events.add(i)
  for i in set_events:
    count_list=[]
    count_list.append(i)
  for i in range(len(count_list)):
    max_event_num=[]
    max_event_num.append(event_id.count(count_list[i]))
  print("Event ID: ", count_list[max_event_num.index(max(max_event_num))], "has the highest number of tickets", max(max_event_num))

